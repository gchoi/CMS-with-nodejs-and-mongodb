form(name = 'userlist', method = 'post', action='/user_information') 
	div(style='width:760px;')
		table
			tr
				th.content ID
				th.content NAME
				th.content Role
			-for(var i=0; i<result.length; i++)
				tr	
					td.content 
						input(type='submit', 
						name = 'user_id', 
						id='user_id', 
						value='#{result[i].Id}') 
					td.content #{result[i].name}
					td.content #{result[i].role}
